{
  "data_owner_name": {
    "type": "input",
    "inputType": "text",
    "modelKey": "data_owner_name",
    "formId": "filplus_application",
    "label": "Data Owner Name",
    "placeholder": "ABC Inc.",
    "tooltip": "Provide the name of your group or organization.",
    "required": true,
    "pre": "[^\\u0000-\\u00ff]",
    "autocomplete": "none",
    "defaultValue": "",
    "chars": {
      "min": 2,
      "max": 1000
    },
    "validationMessage": {
      "required": "This field is required",
      "chars": "Input must be between 2 and 35 characters"
    }
  },
  "your_role": {
    "type": "select",
    "modelKey": "your_role",
    "formId": "filplus_application",
    "label": "What is your role related to the dataset?",
    "required": true,
    "output": "option",
    "validationMessage": {
      "required": "This field is required"
    },
    "options": [
      { "label": "Dataset Owner" },
      { "label": "Data Preparer" },
      { "label": "Storage provider filling out application on behalf of the data owner" },
      { "label": "Data onramp entity that provides data onboarding services to multiple clients" },
      { "label": "Other" }
    ],
    "defaultValue": "",
    "isSingleOption": true
  },
  "total_datacap_size_range": {
    "type": "range",
    "modelKey": "total_datacap_size_range",
    "formId": "filplus_application",
    "required": true,
    "autocomplete": "off",
    "min": 1099511627776,
    "max": 16888498602639360,
    "defaultValue": 1099511627776,
    "intervals": [
      1099511627776,
      68719476736,
      137438953472,
      1099511627776,
      4398046511104,
      17592186044416,
      70368744177664,
      109951162777600,
      281474976710656,
      1125899906842624
    ],
    "react": [
      {
        "modelKey": "total_datacap_size_input",
        "func": "$convertDatacapValueToRangeBytes",
        "args": {
          "valueFromField": "total_datacap_size_input",
          "unitFromField": "total_datacap_size_unit"
        }
      },
      {
        "modelKey": "total_datacap_size_unit",
        "func": "$convertDatacapValueToRangeBytes",
        "args": {
          "valueFromField": "total_datacap_size_input",
          "unitFromField": "total_datacap_size_unit"
        }
      }
    ],
    "validationMessage": {
      "required": "This field is required"
    }
  },
  "total_datacap_size_input": {
    "type": "input",
    "inputType": "number",
    "modelKey": "total_datacap_size_input",
    "formId": "filplus_application",
    "label": "Total Datacap being requested",
    "placeholder": "32",
    "min": 1,
    "max": 5242880,
    "required": true,
    "defaultValue": 1,
    "pre": "[^\\u0000-\\u00ff]",
    "autocomplete": "off",
    "validationMessage": {
      "required": "This field is required",
      "minmax": "Number must be between 1 and 5242880"
    },
    "react": [{
      "modelKey": "total_datacap_size_range",
      "func": "$convertDatacapSizeBytesToHumanNumber",
      "args": {
        "valueFromField": "total_datacap_size_range"
      }
    }]
  },
  "total_datacap_size_unit": {
    "type": "select",
    "modelKey": "total_datacap_size_unit",
    "formId": "filplus_application",
    "label": "Unit",
    "required": true,
    "output": "option",
    "defaultValue": "TiB",
    "isSingleOption": true,
    "validationMessage": {
      "required": "This field is required"
    },
    "options": [
      { "label": "GiB" },
      { "label": "TiB" },
      { "label": "PiB" }
    ],
    "react": [{
      "modelKey": "total_datacap_size_range",
      "func": "$reactUnitToDatacapSizeBytes",
      "args": {
        "valueFromField": "total_datacap_size_range",
        "unitFromField": "total_datacap_size_unit"
      }
    }]
  },
  "ecosystem_associates_radio": {
    "type": "radio",
    "modelKey": "ecosystem_associates_radio",
    "formId": "filplus_application",
    "label": "Ecosystem associates",
    "description": "Is this project associated with ecosystem stakeholders or other relevant projects?",
    "required": true,
    "output": "option",
    "validationMessage": {
      "required": "This field is required"
    },
    "options": [
      { "label": "Yes" },
      { "label": "No" }
    ],
    "defaultValue": ""
  },
  "ecosystem_associates_textarea": {
    "type": "textarea",
    "modelKey": "ecosystem_associates_textarea",
    "formId": "filplus_application",
    "placeholder": "Please provide the other projects or ecosystem stakeholders",
    "pre": "[^\\u0000-\\u00ff]",
    "autocomplete": "none",
    "required": true,
    "conditions": [
      {
        "modelKey": "ecosystem_associates_radio",
        "eq": 0
      }
    ],
    "tooltip": "",
    "chars": {
      "min": 2,
      "max": 1000
    },
    "validationMessage": {
      "required": "This field is required",
      "chars": "Input must be between 2 and 35 characters"
    },
    "defaultValue": ""
  },
  "confirm_follow_fil_guideline": {
    "type": "checkbox",
    "modelKey": "confirm_follow_fil_guideline",
    "formId": "filplus_application",
    "label": "Have you read and do you agree to the Fil+ guidelines?",
    "required": true,
    "output": "option",
    "validationMessage": {
      "required": "This field is required"
    },
    "options": [
      { "label": "Yes" }
    ],
    "defaultValue": ""
  }
}
